{
  "name": "salon-uber-style",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite build && node server.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "seed": "node scripts/seedSupabase.js",
    "migrate:salon-consumption": "node --loader ts-node/esm src/utils/supabase/directTableCreation.js",
    "vercel-build": "npm install @jridgewell/trace-mapping@0.3.25 --save-dev && echo \"Environment check...\" && node -e \"console.log('VITE_SUPABASE_URL:', process.env.VITE_SUPABASE_URL)\" && vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
